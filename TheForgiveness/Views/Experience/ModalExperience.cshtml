@{
    ViewBag.Title = "Tus Ideales";
    Layout = "~/Views/Shared/DashBoard.cshtml";
    ViewBag.Style = "../Assets/Css/Subject/create.min.css";
}

    <div class="modal" tabindex="-1" role="dialog" id="modal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Victimologias a trabajar</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p>Las vitimologia en la cual encaja segun nuestro agente inteligente es:</p>

                    <a href="@Url.Action("GetVictimologys", "Victimology")" class="btn btn-outline-primary">@ViewBag.victimologia</a>
                    <div>
                        <p>Tenga en cuenta que selesugiera visitar :</p>
                        <a href="@Url.Action("GetVictimologys", "Victimology")" class="btn btn-outline-primary">@ViewBag.sugerencia</a>
                    </div>
                </div>

                <div class="modal-footer">
                    <a href="http://pruebamildtreh.epizy.com/Proto/" class="btn btn-secondary">Cerrar</a>
              
                </div>
            </div>
        </div>
    </div>


<script>
    $(document).ready(() => {
        $("#modal").modal()
    });
</script>