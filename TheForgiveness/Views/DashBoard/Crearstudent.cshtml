
@{
    ViewBag.Title = "Registrar Estudiante";
    Layout = "~/Views/Shared/DashBoard.cshtml";
    ViewBag.Style = "";
    ViewBag.Js = "../../Assets/Js/DashBoard/wizard.js";
}



<div class="card-body wizard-content">
    <h4 class="card-title">Complete el formulario para registrar el estudiante</h4>
    <h6 class="card-subtitle"></h6>
    <form id="example-form" action="#" class="m-t-40">
        <div>
            <h3>Personal</h3>
            <section>
                <div class="row">
                    <label for="name">Primer Nombre</label>
                    <input id="name" name="name" type="text" class="required form-control col-sm-5">
                    <label for="name">Segundo Nombre</label>
                    <input id="name" name="name" type="text" class="required form-control col-sm-5">
                </div>
                <div class="row">
                    <label for="surname">Primer Apellido</label>
                    <input id="surname" name="surname" type="text" class="required form-control col-sm-5">
                    <label for="surname">Segundo Apellido</label>
                    <input id="surname" name="surname" type="text" class="required form-control ">
                </div>
                <label for="email">Identificacion!</label>
                <input id="email" name="email" type="text" class="required form-control ">
                <label for="address">Address</label>
                <input id="address" name="address" type="text" class=" form-control ">
                <p>(*) Mandatory</p>
            </section>
            <h3>Contacto</h3>
            <section>
                <label for="name">Numero Telefonico</label>
                <input id="name" name="name" type="text" class="required form-control">
                <label for="surname">E-Mail</label>
                <input id="email" name="email" type="text" class="required email form-control">
                
            </section>
            <h3>Finish</h3>
            <section>
                <input id="acceptTerms" name="acceptTerms" type="checkbox" class="required">
                <label for="acceptTerms">I agree with the Terms and Conditions.</label>
            </section>
        </div>
    </form>
</div>

